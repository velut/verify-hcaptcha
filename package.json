{
  "name": "verify-hcaptcha",
  "version": "0.1.0",
  "author": "Edoardo Scibona",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/verify-hcaptcha.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "test": "dts test",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/verify-hcaptcha.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/verify-hcaptcha.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {},
  "devDependencies": {
    "@size-limit/preset-small-lib": "5.0.5",
    "dts-cli": "0.17.1",
    "husky": "7.0.2",
    "prettier": "2.4.1",
    "size-limit": "5.0.5",
    "tslib": "2.3.1",
    "typescript": "4.4.3"
  }
}
